{
  "version": 2,
  "env": {
    "NEXT_PUBLIC_API_URL": "https://roman-sadia-todo-app.hf.space"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://roman-sadia-todo-app.hf.space/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}